{
  "name": "markdown-shiki",
  "version": "0.1.1",
  "displayName": "Markdown Preview VS Code Highlighting",
  "description": "Use VS Code's highlighter for code blocks in the built-in markdown preview.",
  "icon": "resources/logo.png",
  "keywords": [
    "markdown",
    "highlighting",
    "preview",
    "shiki",
    "code",
    "code block"
  ],
  "publisher": "bierner",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mjbvz/vscode-markdown-shiki.git"
  },
  "bugs": {
    "url": "https://github.com/mjbvz/vscode-markdown-shiki/issues"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "activationEvents": [],
  "main": "./index.js",
  "categories": [
    "Other"
  ],
  "contributes": {
    "markdown.markdownItPlugins": true,
    "configuration": {
      "title": "Markdown Shiki",
      "properties": {
        "markdownShiki.theme": {
          "markdownDescription": "Shiki theme to use in the markdown Preview. When set to `null` - uses active color theme.",
          "scope": "window",
          "type": [
            "string",
            "null"
          ],
          "enum": [
            null,
            "abyss",
            "dark_plus",
            "dark_vs",
            "hc_black",
            "kimbie_dark",
            "light_plus",
            "light_vs",
            "monokai",
            "monokai_dimmed",
            "quietlight",
            "red",
            "solarized_dark",
            "solarized_light",
            "Material-Theme-Darker-High-Contrast",
            "Material-Theme-Darker",
            "Material-Theme-Default-High-Contrast",
            "Material-Theme-Default",
            "Material-Theme-Lighter-High-Contrast",
            "Material-Theme-Lighter",
            "Material-Theme-Ocean-High-Contrast",
            "Material-Theme-Ocean",
            "Material-Theme-Palenight-High-Contrast",
            "Material-Theme-Palenight",
            "nord",
            "min-light",
            "min-dark",
            "white",
            "white-night",
            "zeit"
          ],
          "default": null
        }
      }
    }
  },
  "dependencies": {
    "oniguruma": "^7.2.0",
    "shiki": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^11.9.4",
    "@types/vscode": "^1.38"
  },
  "scripts": {}
}
